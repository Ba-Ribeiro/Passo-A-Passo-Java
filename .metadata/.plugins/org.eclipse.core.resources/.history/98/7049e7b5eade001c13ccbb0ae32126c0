package br.com.entra21.PassoAPasso.Java.OperadoresCondicionais;
public class Pre {
	public static void pre(String[] args) {

		// Pré
		int a = 10;

		++a; // é um atalho para adiciona +1 ao valor já existente na variavel a. logo
				// ficaria 1+10=11

		// Pós
		int b = 10;

		b++; // é um atalho para adiciona +1 ao valor já existente na variavel a. logo
				// ficaria 1+10=11

		// Exemplo?

		int c = 10;
		int d = c++;

		System.out.println(c);
		System.out.println(d);

	}
}
